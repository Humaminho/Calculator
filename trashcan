function operate() {
    if (firstVal ==0 && secondVal == 0) {
        firstVal = 0;
        console.log("working");
    } else {
        storeVal();
        resolveFunc(firstVal, secondVal);
        render();
        firstVal = displayVal;
        displayVal = 0;
        secondVal = 0;
        resolveFunc = function() {
            return "empty";
        };
    };
};